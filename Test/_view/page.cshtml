<!DOCTYPE html>
<html>
<head>
    @Input("_view/helpers")
    <title>@Model.title</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <div class="container">

        @RenderBody()
        
        <ul>
        @foreach(var food in (List<string>)Model.foods)
        {
            <li>@food</li>
        }
        </ul>

        @SayHi("Hello")
        @if (IsSectionDefined("Well"))
        {
            <section>
                <div class="well">
                    @RenderSection("Well")
                </div>
            </section>
        }


    </div>
    

</body>
</html>