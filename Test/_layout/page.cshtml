<!DOCTYPE html>
<html>
<head>
    @Input("_helpers")
    <title>@Model.title</title>
    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/bootstrap-theme.css">
</head>
<body>
    <div class="container">
        @RenderBody()


        @SayHi("Hello")
        @if (IsSectionDefined("Well"))
        {
            <section>
                <div class="well">
                    @RenderSection("Well")
                </div>
            </section>
        }
    </div>
    

</body>
</html>